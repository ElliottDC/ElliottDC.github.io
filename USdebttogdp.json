{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Google's stock price over time.",
    "data": {"url": "https://raw.githubusercontent.com/ElliottDC/ElliottDC.github.io/main/US%20debt%20to%20GDP.csv",
    "format":{"type":"csv" }},

    "title": {
        "text": "US Debt to GDP",
